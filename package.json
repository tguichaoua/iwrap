{
    "name": "iwrap",
    "version": "0.1.0",
    "description": "A wrapper around Iterable to bring access to powerful methods similar to `filter`, `map`, etc ...",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "npm run lint:fix && npm run test:build && npm run mocha",
        "test:build": "tsc --noEmit",
        "mocha": "mocha -r ts-node/register 'src/**/*.spec.ts'",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "build": "rimraf ./dist && tsc",
        "prepack": "npm run build",
        "prepare": "is-ci || husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tguichaoua/iwrap.git"
    },
    "bugs": {
        "url": "https://github.com/tguichaoua/iwrap/issues"
    },
    "keywords": [
        "iterable",
        "utility",
        "iterator",
        "generator"
    ],
    "author": {
        "name": "Tristan Guichaoua",
        "email": "tguichaoua.dev@gmail.com"
    },
    "license": "MIT",
    "files": [
        "dist/*"
    ],
    "devDependencies": {
        "@types/chai": "^4.2.21",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.4.7",
        "@typescript-eslint/eslint-plugin": "^4.28.4",
        "@typescript-eslint/parser": "^4.28.4",
        "chai": "^4.3.4",
        "eslint": "^7.31.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^7.0.0",
        "is-ci": "^3.0.0",
        "lint-staged": "^11.1.1",
        "mocha": "^9.0.3",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5"
    }
}
